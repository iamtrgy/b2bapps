{
  "$schema": "https://schema.tauri.app/config/2/capability",
  "identifier": "default",
  "description": "Default capability for the POS app",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:webview:allow-print",
    "store:default",
    "http:default",
    "os:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://**"
        }
      ]
    }
  ]
}
